<Window x:Class="TabletManagerWPF.View.Printer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TabletManagerWPF.View"
        mc:Ignorable="d"
        Title="Printer" Height="327" Width="376" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <WebBrowser x:Name="WebBrowser" Margin="0,38,0,-1" />
        <Button x:Name="button" Content="" Style="{StaticResource ButtonStyle}" HorizontalAlignment="Left" Margin="6,2,0,0" VerticalAlignment="Top" Height="31" Width="34" Click="button_Click" BorderBrush="{x:Null}">
            <Button.Background>
                <ImageBrush ImageSource="/Resources/printer.png"/>
            </Button.Background>
        </Button>
        <Image Source="/Resources/printer.png" MouseDown="Image_MouseDown" HorizontalAlignment="Right" Width="36" Height="33" VerticalAlignment="Top" RenderTransformOrigin="-0.258,0.511" Margin="0,2,10,0"/>

    </Grid>
</Window>
